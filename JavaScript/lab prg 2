<script>
  count = 0
  sum = 0
	n = parseFloat(prompt("Enter the values"))
	while(n != 0){
    count ++
    sum = sum + n
	n = parseFloat(prompt("Enter the values"))
   }
   alert("this is sum " + sum)
   avg = sum / count;
   alert("this is avg " +avg)
</script>



//prgrm to display number squre cube
<script>
	str = "number squre cube \n"
	val = parseInt(prompt("Enter The Value "))
	sq = val * val
	cub = val * val * val
	str = str +" " +val+" "+ sq+" " +cub  
	alert(str)
</script>
